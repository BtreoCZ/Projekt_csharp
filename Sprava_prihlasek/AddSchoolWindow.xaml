<Window x:Class="Sprava_prihlasek.AddSchoolWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sprava_prihlasek"
        mc:Ignorable="d"
        Title="AddSchoolWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Nazev" HorizontalAlignment="Left" Margin="152,138,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="218"/>
        <TextBox x:Name="Mesto" HorizontalAlignment="Left" Margin="152,192,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="218"/>
        <TextBox x:Name="Adresa" HorizontalAlignment="Left" Margin="152,246,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="218"/>
        <Label Content="Nazev" HorizontalAlignment="Left" Margin="152,107,0,0" VerticalAlignment="Top"/>
        <Label Content="Mesto" HorizontalAlignment="Left" Margin="152,161,0,0" VerticalAlignment="Top"/>
        <Label Content="Adresa" HorizontalAlignment="Left" Margin="152,215,0,0" VerticalAlignment="Top"/>
        <Label Content="Přidání střední školy" HorizontalAlignment="Left" Margin="28,23,0,0" VerticalAlignment="Top" Height="65" Width="372" FontSize="24"/>
        <Button Content="Přidat školu" HorizontalAlignment="Left" Margin="302,355,0,0" VerticalAlignment="Top" Click="AddSchoolToDB" Height="48" Width="136"/>

    </Grid>
</Window>
