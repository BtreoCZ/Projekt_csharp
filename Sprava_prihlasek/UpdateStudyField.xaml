<Window x:Class="Sprava_prihlasek.UpdateStudyField"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sprava_prihlasek"
        mc:Ignorable="d"
        Title="UpdateStudyField" Height="450" Width="400">
    <Grid>
        <ComboBox x:Name="StudyFieldComboBox" HorizontalAlignment="Center" Margin="0,113,0,0" VerticalAlignment="Top" Width="300"/>
        <Label Content="Vyberte studijní plán" HorizontalAlignment="Left" Margin="50,82,0,0" VerticalAlignment="Top" Width="108"/>
        <Button Content="Zrušit" HorizontalAlignment="Left" Margin="63,0,0,0" VerticalAlignment="Center" Height="40" Width="80" Click="Cancel"/>
        <Button Content="Aktualizovat" HorizontalAlignment="Left" Margin="236,0,0,0" VerticalAlignment="Center" Height="40" Width="80" Click="Update"/>

    </Grid>
</Window>
